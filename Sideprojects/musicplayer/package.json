{
  "name": "musicplayer",
  "productName": "Music Player",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . musicplayer --overwrite --asar=true --platform=win32 --arch=x64 --icon=ico/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Music Player\""
  },
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "node-static": "^0.7.11"
  }
}
